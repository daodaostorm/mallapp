<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="BallFadeLoadingView">
        <attr name="loadingViewColor" format="color" />

    </declare-styleable>

    <declare-styleable name="CircularProgressView">
        <attr name="backWidth" format="dimension" />    <!--背景圆环宽度-->
        <attr name="progWidth" format="dimension" />    <!--进度圆环宽度-->
        <attr name="backColor" format="color" />        <!--背景圆环颜色-->
        <attr name="progColor" format="color" />        <!--进度圆环颜色-->
        <attr name="progStartColor" format="color" />   <!--进度圆环开始颜色-->
        <attr name="progFirstColor" format="color" />   <!--进度圆环结束颜色-->
        <attr name="progress" format="integer" />       <!--圆环进度-->
    </declare-styleable>

    <declare-styleable name="RingProgressBar">
        <attr name="rRingColor" format="color" />
        <attr name="rTextColor" format="color" />
        <attr name="rTextSize" format="dimension" />
        <attr name="rCircleColor" format="color" />
        <attr name="rProgressColor" format="color" />
        <attr name="rMax" format="integer" />
        <attr name="rTextIsVisibility" format="boolean" />
        <attr name="style">
            <enum name="STROKE" value="0" />
            <enum name="FILL" value="1" />
        </attr>
    </declare-styleable>
</resources>